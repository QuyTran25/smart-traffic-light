        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <!-- =====================================================
         1️⃣ ĐỊNH NGHĨA CÁC LOẠI PHƯƠNG TIỆN
         ===================================================== -->

    <!-- Xe máy (80%) -->
    <vType id="motorcycle" vClass="motorcycle"
           length="2.0" width="0.8" minGap="0.2"
           accel="2.0" decel="4.5" sigma="0.8"
           maxSpeed="20" speedFactor="1.2"
           color="1,0,0"/>

    <!-- Ô tô (10%) -->
    <vType id="car" vClass="passenger"
           length="4.5" width="1.8" minGap="2.5"
           accel="2.6" decel="4.5" sigma="0.5"
           maxSpeed="25" speedFactor="1.0"
           color="0,0,1"/>

    <!-- Xe buýt (7%) -->
    <vType id="bus" vClass="bus"
           length="10" width="2.5" minGap="3.0"
           accel="1.2" decel="4.0" sigma="0.4"
           maxSpeed="18" speedFactor="0.9"
           color="0,1,0"/>

    <!-- Xe ưu tiên (3%) -->
    <vType id="priority" vClass="emergency"
           length="4.5" width="1.8" minGap="2.0"
           accel="3.0" decel="4.5" sigma="0.3"
           maxSpeed="30" speedFactor="1.3"
           color="1,1,0"/>

